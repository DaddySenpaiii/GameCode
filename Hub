    if v.UserId == 77832099 then
		wait(1)
		OrionLib:MakeNotification({
			Name = "trusted User",
			Content = "No need for a key for you :)",
			Image = "rbxassetid://4483345998",
			Time = 5
		})
		wait(1)
		MakeScriptHub()
    end
 end

