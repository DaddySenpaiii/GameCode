Tab:AddButton({
	Name = "LX63 Hub",
	Callback = function()
		OrionLib:MakeNotification({
			Name = "Required Hats",
			Content = "Copied to clipboard   https://discord.gg/ZNmRM6XS7R",
			Image = "rbxassetid://4483345998",
			Time = 5
		})
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/Gogogamer61/LXHub-Main/main/LXHub%20Main%20Script'),true))()
		setclipboard("https://discord.com/invite/SZWDr6YyZ4")
toclipboard("https://discord.com/invite/SZWDr6YyZ4")

      		print("button pressed")
  	end    
})

